{
  "name": "@guardx/frontend",
  "version": "1.0.0",
  "description": "GuardX frontend React application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
